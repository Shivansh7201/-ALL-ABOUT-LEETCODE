   ],
   "source": [
    "grades = [85, 90, 92, 88, 95]  # list of grades\n",
    "total = sum(grades)  # calculate the sum of grades\n",
    "num_grades = len(grades)  # get the number of grades\n",
    "gpa = total / num_grades  # calculate the GPA\n",
    "print(\"GPA:\", gpa)"
   ]
  }
